/*
Build DAPSO
*/
begin dapso:DReg
    set particles 100
    set pos_bound 1.0
    set vel_c1 0.4
    set vel_c2 0.2
end
/* Build regressor */
begin ann:reg
    set type regressor
    set neurons [8,15]
    set data ["abalone_train.csv","abalone_test.csv"]
    set algorithm DReg
end
/* Execute for different iterations */
fit reg 50
out reg "reg_50.csv"
fit reg 100
out reg "reg_100.csv"